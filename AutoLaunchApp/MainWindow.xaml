<Window x:Class="AutoLaunchApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AutoLaunchApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel>
        
        <TextBlock>AutoLaunch</TextBlock>
        <CheckBox IsChecked="{Binding IsChecked,Mode=TwoWay}"></CheckBox>
        <TextBlock Text="{Binding State}"></TextBlock>
    </StackPanel>
</Window>
